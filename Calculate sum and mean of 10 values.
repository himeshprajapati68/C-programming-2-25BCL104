#include <stdio.h>

int main() {
    int i;
    float numbers[10], sum = 0.0, mean;

    printf("Enter 10 numbers:\n");
    for(i = 0; i < 10; i++) {
        scanf("%f", &numbers[i]);
        sum += numbers[i];  // Add each number to sum
    }

    mean = sum / 10;  // Calculate mean

    printf("Sum of the 10 numbers is: %.2f\n", sum);
    printf("Mean of the 10 numbers is: %.2f\n", mean);

    return 0;
}
