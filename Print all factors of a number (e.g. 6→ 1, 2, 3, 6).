#include <stdio.h>

int main() {
    int num, i;

    printf("Enter a number: ");
    scanf("%d", &num);

    if(num <= 0) {
        printf("Please enter a positive number.\n");
        return 0;
    }

    printf("Factors of %d are: ", num);

    for(i = 1; i <= num; i++) {
        if(num % i == 0) {  // Check if i divides num completely
            printf("%d ", i);
        }
    }

    printf("\n");

    return 0;
}
