#include <stdio.h>

int main() {
    int i, j, isPrime;

    printf("Prime numbers between 1 and 500 are:\n");

    for(i = 2; i <= 500; i++) {  // Start from 2
        isPrime = 1;  // Assume i is prime

        // Check for divisors from 2 to âˆši
        for(j = 2; j * j <= i; j++) {
            if(i % j == 0) {
                isPrime = 0;
                break;
            }
        }

        if(isPrime) {
            printf("%d ", i);
        }
    }

    printf("\n");
    return 0;
}
